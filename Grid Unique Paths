class Solution {
    static int f(int i,int j,int[][] dp){
        if(i<0 || j<0) return 0;
        if(i==0 && j==0) return 1;
        if(dp[i][j]!=-1) return dp[i][j];
        int left = f(i,j-1,dp);
        int up = f(i-1,j,dp);
        return dp[i][j] = left+up;
    }
    public int uniquePaths(int m, int n) {
       int[][] dp = new int[m+1][n+1];
       for(int[] row : dp) Arrays.fill(row,-1);
       return f(m-1,n-1,dp);
    }
}
